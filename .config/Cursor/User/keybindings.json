[
  {
    "key": "space ,",
    "command": "workbench.action.showAllEditors",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space space",
    "command": "workbench.action.quickOpen",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space space",
    "command": "workbench.action.quickOpen",
    "when": "!editorTextFocus && neovim.mode == normal && !sideBarFocus && !inputFocus && !inQuickOpen"
  },
  {
    "key": "space b b",
    "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space b b",
    "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
    "when": "neovim.mode == normal && !neovim.recording && !inputFocus"
  },
  {
    "key": "space /",
    "command": "workbench.action.findInFiles",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "ctrl+/",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "space b d",
    "command": "workbench.action.closeActiveEditor",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "ctrl+w",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "key": "space e",
    "command": "workbench.view.explorer",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording && !sideBarVisible"
  },
  {
    "key": "space e",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "neovim.mode == normal && !neovim.recording && sideBarVisible && !inputFocus"
  },
  {
    "key": "space f m",
    "command": "workbench.view.explorer",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space s s",
    "command": "workbench.action.showAllSymbols",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "ctrl+b",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "space c a",
    "command": "editor.action.quickFix",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space v",
    "command": "workbench.action.splitEditorRight",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space h",
    "command": "workbench.action.splitEditorDown",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space -",
    "command": "workbench.action.splitEditorDown",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space w d",
    "command": "workbench.action.closeEditorsInGroup",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "ctrl+x",
    "command": "workbench.actions.view.toggleProblems"
  },
  {
    "args": "<C-d>",
    "command": "vscode-neovim.send",
    "key": "ctrl+d",
    "when": "editorFocus && neovim.init"
  },
  {
    "key": "ctl+h",
    "command": "workbench.action.navigateLeft"
  },
  {
    "key": "ctl+j",
    "command": "workbench.action.navigateDown"
  },
  {
    "key": "ctl+k",
    "command": "workbench.action.navigateUp"
  },
  {
    "key": "ctl+l",
    "command": "workbench.action.navigateRight"
  },
  {
    "key": "space g g",
    "command": "extension.multiCommand.closeSidebarAndOpenLazyGit",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space g g",
    "command": "extension.multiCommand.closeBothSidebarsAndOpenLazyGit",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording && auxiliaryBarVisible"
  },
  {
    "key": "space c r",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && neovim.mode == normal && !neovim.recording && !editorReadonly"
  },
  {
    "key": "space g r",
    "command": "editor.action.referenceSearch.trigger",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space t s",
    "command": "workbench.view.testing.focus",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space m p",
    "command": "markdown.showPreviewToSide",
    "when": "editorLangId == 'markdown' && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space s o",
    "command": "outline.focus",
    "when": "!outline.focus && editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space s o",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "sideBarVisible && outline.focus && !inputFocus"
  },
  {
    "key": "space u",
    "command": "timeline.focus",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space f h",
    "command": "timeline.focus",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space u",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "sideBarVisible && timeline.active && !inputFocus"
  },
  {
    "key": "space s r",
    "command": "editor.action.startFindReplaceAction",
    "when": "editorTextFocus && neovim.mode == normal"
  },
  // {
  //   "key": "space t t",
  //   "command": "todo-tree-view.focus",
  //   "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  // },
  {
    "key": "space t t",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "(editorTextFocus || sideBarFocus) && !inputFocus"
  },
  {
    "key": "shift+cmd+i",
    "command": "-workbench.action.quickchat.toggle",
    "when": "chatIsEnabled"
  },
  // {
  //   "key": "cmd+r",
  //   "command": "continue.newSession"
  // },
  {
    "key": "shift+cmd+i",
    "command": "composer.openAsPane"
  },
  {
    "key": "cmd+i",
    "command": "composerMode.agent"
  },
  {
    "key": "ctrl+shift+l",
    "command": "workbench.action.moveEditorToNextGroup",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+h",
    "command": "workbench.action.moveEditorToPreviousGroup",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+k",
    "command": "workbench.action.moveEditorToAboveGroup",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+j",
    "command": "workbench.action.moveEditorToBelowGroup",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+cmd+b",
    "command": "workbench.action.toggleAuxiliaryBar" // Toggle secondary / auxiliary sidebar bar
  },
  {
    "key": "space p n",
    "command": "editor.action.marker.next", // next error
    "when": "!outline.focus && editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space p shift+n",
    "command": "editor.action.marker.nextInFiles", // next error
    "when": "!outline.focus && editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space p p",
    "command": "editor.action.marker.prev", // previous error
    "when": "!outline.focus && editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space p shift+p",
    "command": "editor.action.marker.prevInFiles", // previous error
    "when": "!outline.focus && editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.focusRightGroup",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.focusLeftGroup",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space g b l",
    "command": "gitlens.toggleLineBlame",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space g b f",
    "command": "gitlens.toggleFileBlame",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
  {
    "key": "space g h",
    "command": "workbench.scm.history.focus",
    "when": "editorTextFocus && neovim.mode == normal && !neovim.recording"
  },
]